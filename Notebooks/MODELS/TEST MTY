rint(f\"✅ Successfully combined the CSV files into {output_csv_path}\")\n","else:\n","    print(\"❌ No CSV files were found or successfully loaded. No combined file was created.\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"gioCEvOBONSd","executionInfo":{"status":"ok","timestamp":1752169042071,"user_tz":-120,"elapsed":72,"user":{"displayName":"Joaquin Garcia Luna Perez","userId":"01168812787878955692"}},"outputId":"a11369f6-e573-412a-9d68-117ff7e52f51"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["✅ Successfully combined the CSV files into /content/MTY_base_datos_unida.csv\n"]}]}]}
pagate theme\n","            rack_id = f\"{rack_prefix}{rack_counter:04d}\"\n","            for r in current_rack:\n","                r[\"rack_id\"] = rack_id\n","                r[\"temas_directos\"] = current_moderator_theme or [\"Sin Clasificar\"] # Propagate theme from the *previous* moderator\n","            new_rows.extend(current_rack)\n","            rack_counter += 1\n","            # --> Añadir reset del tema aquí después de completar un rack por nuevo moderador <--\n","            current_moderator_theme = [] # Reset theme\n","\n","        # Reset for the new rack started by the moderator\n","        current_rack = []\n","        current_token_sum = 0\n","        # Set the new theme base for the rack starting with this moderator\n","        # --> El tema del nuevo moderador se asigna inmediatamente después del reset <--\n","        current_moderator_theme = row[\"temas_detectados\"]\n","\n","\n","    # Always append the current row to the current_rack\n","    current_rack.append(row.to_dict())\n","    current_token_sum += row[\"num_tokens\"]\n","\n","    # Check if the current rack exceeds the max_tokens\n","    if current_token_sum >= max_tokens:\n","        # If the current rack exceeds max_tokens, save it if it meets the min_tokens size\n","        if current_token_sum >= min_tokens:\n","            # Save as valid rack if it meets min size\n","            rack_id = f\"{rack_prefix}{rack_counter:04d}\"\n","            for r in current_rack:\n","                r[\"rack_id\"] = rack_id\n","                r[\"temas_directos\"] = current_moderator_theme or [\"Sin Clasificar\"]\n","            new_rows.extend(current_rack)\n","            rack_counter += 1\n","            # --> Añadir reset del tema aquí después de completar un rack por max_tokens <--\n","            current_moderator_theme = [] # Reset theme\n","\n","        # Reset for the next potential rack (regardless of min_tokens check after max_tokens reached)\n","        current_rack = []\n","        current_token_sum = 0\n","\n","\n","# After the loop finishes, check if the last accumulated rack is valid\n","if current_rack and current_token_sum >= min_tokens:\n","    rack_id = f\"{rack_prefix}{rack_counter:04d}\"\n","    for r in current_rack:\n","        r[\"rack_id\"] = rack_id\n","        r[\"temas_directos\"] = current_moderator_theme or [\"Sin Clasificar\"]\n","    new_rows.extend(current_rack)\n","    # --> Añadir reset del tema aquí después de completar el último rack <--\n","    current_moderator_theme = [] # Reset theme\n","    # No need to increment rack_counter or reset here, the process is done.\n","\n","# Final DataFrame - only includes rows that were part of a valid rack (>= min_tokens)\n","df_rag_final = pd.DataFrame(new_rows)\n","\n","# Add the accumulated token count column - This part remains the same\n","df_rag_final[\"tokens_acumulados\"] = df_rag_final.groupby(\"rack_id\")[\"num_tokens\"].cumsum()\n","\n"],"metadata":{"id":"EckrIOwQGg-z","executionInfo":{"status":"ok","timestamp":1750102632685,"user_tz":-60,"elapsed":521,"user":{"displayName":"Joaquin Garcia Luna Perez","userId":"01168812787878955692"}}},"execution_count":23,"outputs":[]},{"cell_type":"code","source":["df_rag_final.head(20)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"y4riTr4OLx1Z","executionInfo":{"status":"ok","timestamp":1750102696987,"user_tz":-60,"elapsed":65,"user":{"displayName":"Joaquin Garcia Luna Perez","userId":"01168812787878955692"}},"outputId":"82358521-5144-460e-e213-d4cc7b1752c0"},"execution_count":24,"outputs":[{"output_type":"execute_result","data":{"text/plain":["   Grupo Plaza Estado   Edades   NSE                      Participante  \\\n","0    SG1  CDMX   CDMX  18 a 25  C-D+                 Natalia Rodríguez   \n","1    SG1  CDMX   CDMX  18 a 25  C-D+                 Antonieta Murillo   \n","2    SG4  CDMX   CDMX  18 a 25  C-D+                 Natalia Rodríguez   \n","3    SG4  CDMX   CDMX  18 a 25  C-D+       